	2 [label="Node 2"]
	3 [label="Node 3"]
	4 [label="Node 4" color=blue shape=doublecircle]
	5 [label="Node 5" color=blue shape=doublecircle]
	0 -> 1 [label="0/10"]
	0 -> 2 [label="9/10"]
